BrowserMatch MSIE ie
Header set X-UA-Compatible "IE=Edge,chrome=1" env=ie

RewriteEngine On

# pass all the non uploads and non admin requests to frontend application
RewriteCond %{REQUEST_URI} !^/(basicdemo/uploads)
RewriteCond %{REQUEST_URI} !^/(basicdemo/backend/web|basicdemo/backend)
RewriteRule !(frontend/web) /basicdemo/frontend/web%{REQUEST_URI} [L]

# pass all the admin requests to admin application
RewriteCond %{REQUEST_URI} ^/(basicdemo/backend)
RewriteRule !(backend/web) /basicdemo/backend/web%{REQUEST_URI} [L]

# handle frontend application requests
RewriteCond %{REQUEST_URI} ^/basicdemo/frontend/web
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)\?*$ /basicdemo/frontend/web/index.php/$1 [L,QSA]

# handle admin application requests
RewriteCond %{REQUEST_URI} ^/basicdemo/backend/web
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)\?*$ /basicdemo/backend/web/index.php/$1 [L,QSA]
